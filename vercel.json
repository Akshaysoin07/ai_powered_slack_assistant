{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "/(.*.js)",
      "use": "@vercel/static"
    },
    {
      "src": "/(.*.css)",
      "use": "@vercel/static"
    },
    {
      "src": "/(.*.html)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/(.*.js)",
      "headers": {
        "Content-Type": "text/javascript"
      },
      "continue": true
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
